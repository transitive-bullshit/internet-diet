{
  "name": "internet-diet",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "description": "Chrome extension to remove unhealthy food options from the web.",
  "repository": "transitive-bullshit/internet-diet",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "license": "MIT",
  "keywords": [
    "chrome",
    "extension",
    "diet"
  ],
  "scripts": {
    "start": "run-s watch",
    "build": "run-p build:* --continue-on-error",
    "build:typescript": "tsc --noEmit",
    "build:webpack": "cross-env NODE_OPTIONS=\"--loader ts-node/esm --no-warnings\" webpack --mode=production",
    "test": "run-s test:*",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check",
    "watch": "run-p watch:* --continue-on-error",
    "watch:typescript": "tsc --noEmit --watch --preserveWatchOutput",
    "watch:webpack": "cross-env NODE_OPTIONS=\"--loader ts-node/esm --no-warnings\" webpack --mode=development --watch"
  },
  "dependencies": {
    "lodash.throttle": "^4.1.1",
    "normalize-url": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "select-dom": "^7.1.1"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^2.0.0",
    "@types/chrome": "^0.0.179",
    "@types/lodash.throttle": "^4.1.6",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "del-cli": "^4.0.1",
    "esbuild-loader": "^2.18.0",
    "fs-extra": "^10.0.1",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "size-plugin": "^2.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "webextension-polyfill": "^0.8.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  }
}
