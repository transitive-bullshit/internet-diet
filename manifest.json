{
  "name": "Internet Diet",
  "description": "Remove unhealthy choices from the web.",
  "version": "0.0.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["*://*.postmates.com/*"],
      "js": ["src/content.js"],
      "run_at": "document_start"
    }
  ]
}
